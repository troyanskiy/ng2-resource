{
  "name": "ngx-resource",
  "version": "2.0.6",
  "description": "Angular2 resource module with simple decorators",
  "scripts": {
    "clear": "rm ./*.d.ts && rm ./*.metadata.json && rm ./index.js && rm ./ngx-resource.js && rm ./src/*.json && rm ./src/*.d.ts && rm ./src/*.js",
    "ngc": "ngc",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "test": "npm run lint && karma start",
    "tslint": "tslint",
    "compile": "ngc && npm run build",
    "build": "webpack && cp bundles/ngx-resource.umd.js bundles/index.js"
  },
  "keywords": [
    "ngx-resource",
    "ngx-resource-rest",
    "ng2-resource",
    "ng2-resource-rest",
    "ng2",
    "ng2-rest",
    "Angular2",
    "Resource"
  ],
  "main": "bundles/ngx-resource.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/troyanskiy/ng2-resource-rest.git"
  },
  "author": "Roman Rosluk <roma@redbox.by>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/troyanskiy/ng2-resource-rest/issues"
  },
  "homepage": "https://github.com/troyanskiy/ng2-resource-rest#readme",
  "peerDependencies": {
    "@angular/core": ">=2.0.0",
    "@angular/common": ">=2.0.0",
    "@angular/http": ">=2.0.0"
  },
  "devDependencies": {
    "@angular/common": "4.0.0",
    "@angular/compiler": "4.0.0",
    "@angular/compiler-cli": "4.0.0",
    "@angular/core": "4.0.0",
    "@angular/http": "4.0.0",
    "@angular/platform-browser": "4.0.0",
    "@angular/platform-browser-dynamic": "4.0.0",
    "@angular/platform-server": "4.0.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "7.0.11",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "3.1.2",
    "clean-webpack-plugin": "0.1.16",
    "codelyzer": "3.0.0-beta.4",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
